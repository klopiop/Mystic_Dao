#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/dist/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/dist/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/tsx@4.21.0/node_modules:/mnt/d/nextcloud/Personal_Doc/比赛&项目存档/比赛/阿里天池/ESA Pages 边缘开发大赛/functions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../tsx/dist/cli.mjs" "$@"
fi
